m.name,\n\tscale: 0.5,\n\tvision: false,\n\twidth: 1\n};\nconst tokenData = await dummyActor.getTokenData(overrideData);\n\nwarpgate.crosshairs.show({\n\ticon: spellEffect,\n\tinterval: -1,\n\trememberControlled: true\n},{\n\tshow: checkDistance\n});\n\nasync function checkDistance(crosshairs) {\n\tconst range = item.data.data.range.value;\n\tconst unit = canvas.scene.data.gridUnits;\n\tlet crosshairsDistance = 0, counter = 0;\n\tif ( token ) {\n\t\twhile ( crosshairs.inFlight && counter < 600 ) {\n\t\t\tcounter++;\n\t\t\tawait warpgate.wait(100);\t// Wait for initial render and delay loop\n\t\t\tconst ray = new Ray(token.center, crosshairs);\n\t\t\tconst distance = canvas.grid.measureDistances([{ray}], {gridSpaces: true});\n\t\t\tif ( distance !== crosshairsDistance ) {\n\t\t\t\tcrosshairsDistance = distance;\n\t\t\t\tcrosshairs.label = `${distance} ${unit}`;\n\t\t\t\tif ( distance > range ) crosshairs.icon = \"icons/svg/hazard.svg\";\n\t\t\t\telse crosshairs.icon = spellEffect;\n\t\t\t\tcrosshairs.draw();\n\t\t\t}\n\t\t}\n\t} else {\n\t\twhile ( crosshairs.inFlight && counter < 600 ) {\n\t\t\tcounter++;\n\t\t\tawait warpgate.wait(100);\n\t\t}\n\t}\n\tif ( counter >= 600 ) crosshairs.inFlight = false;\n\tif ( crosshairs.cancelled ) return;\n\tif ( crosshairsDistance > range ) return ui.notifications.warn(\"The location is out of spell range.\");\n\tawait new Sequence()\n\t\t.sound(\"upload-player/feanor/misty-step.wav\")\n\t\t\t.fadeInAudio(2000)\n\t\t\t.fadeOutAudio(3000)\n\t\t.wait(300)\n\t\t.effect()\n\t\t\t.file(\"jb2a.swirling_sparkles.01.blue\")\n\t\t\t.atLocation(crosshairs, {cacheLocation: true})\n\t\t\t.scale(0.5)\n\t\t.wait(2400)\n\t\t.play();\n\tconst mageHandToken = canvas.tokens.placeables.find(t => t.data.flags.warpgate?.casterActorId === actor.id);\n\tif ( mageHandToken ) warpgate.dismiss(mageHandToken.id);\n\twarpgate.spawnAt(crosshairs, tokenData);\n}","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"