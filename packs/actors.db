 template = canvas.templates.placeables[canvas.templates.placeables.length - 1];\n\tconst templateCenter = {x: template.x + grid * 3, y: template.y + grid * 3};\n\tconst tileData = JSON.parse(`{\"img\":\"${patternImage}\",\"width\":${grid*6},\"height\":${grid*6},\"x\":${template.x},\"y\":${template.y},\"overhead\":true,\"occlusion\":{\"alpha\":0.4,\"mode\":3},\"flags\":{\"tokenmagic\":{\"filters\":[{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"mix66eq324lh0k5y\",\"tmFilterType\":\"glow\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"glow\",\"filterId\":\"hypnoticPattern\",\"color\":773372,\"outerStrength\":2,\"animated\":{\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":773372,\"val2\":16734199}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"mix66eq324lh0k5y\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"uiqm25hwruucpf4q\"}}},{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"ro0gghrmt9bhryoj\",\"tmFilterType\":\"distortion\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"distortion\",\"filterId\":\"hypnoticPattern\",\"animated\":{\"maskSpriteX\":{\"speed\":0.05,\"animType\":\"move\"},\"maskSpriteY\":{\"speed\":0.07,\"animType\":\"move\"}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"ro0gghrmt9bhryoj\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"ok7xjbechdh7p8vf\"}}},{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"7jbcq40dijxw8kal\",\"tmFilterType\":\"shadow\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"shadow\",\"filterId\":\"hypnoticPattern\",\"rotation\":45,\"distance\":70,\"alpha\":0.7,\"color\":212299,\"animated\":{\"blur\":{\"loopDuration\":2000,\"animType\":\"cosOscillation\",\"val1\":1,\"val2\":5},\"rotation\":{\"loopDuration\":4000,\"animType\":\"sinOscillation\",\"val1\":40,\"val2\":50},\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":212299,\"val2\":4987466}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"7jbcq40dijxw8kal\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"mjpre97fk5ry1kc6\"}}}]}}}`);\n\ttemplate.document.delete();\n\tawait new Sequence()\n\t\t.effect()\n\t\t\t.file(\"jb2a.magic_signs.circle.02.illusion.intro.purple\")\n\t\t\t.scale(.75)\n\t\t\t.belowTokens()\n\t\t\t.atLocation(templateCenter)\n\t\t.wait(2600)\n\t\t.play();\n\tcanvas.scene.createEmbeddedDocuments(\"Tile\", [tileData]);\n}\n\nif ( args[0] === \"off\" ) {\n\tconst lastArg = args[args.length - 1];\n\tconst effect = await fromUuid(lastArg.efData.document.uuid);\n\tif ( !effect ) {\n\t\tconst tile = canvas.foreground.tiles.find(t => t.data.img === patternImage);\n\t\tif ( tile ) tile.document.delete();\n\t} else DAE.deleteActiveEffect(lastArg.actorUuid, lastArg.origin);\n}\n\n// Tile's filters backup: '[{\"filterType\":\"glow\",\"filterId\":\"hypnoticPattern\",\"color\":773372,\"outerStrength\":2,\"animated\":{\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":773372,\"val2\":16734199}}},{\"filterType\":\"distortion\",\"filterId\":\"hypnoticPattern\",\"animated\":{\"maskSpriteX\":{\"speed\":0.05,\"animType\":\"move\"},\"maskSpriteY\":{\"speed\":0.07,\"animType\":\"move\"}}},{\"filterType\":\"shadow\",\"filterId\":\"hypnoticPattern\",\"rotation\":45,\"distance\":70,\"alpha\":0.7,\"color\":212299,\"animated\":{\"blur\":{\"loopDuration\":2000,\"animType\":\"cosOscillation\",\"val1\":1,\"val2\":5},\"rotation\":{\"loopDuration\":4000,\"animType\":\"sinOscillation\",\"val1\":40,\"val2\":50},\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":212299,\"val2\":4987466}}}]'","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}}},"img":"icons/magic/air/air-burst-spiral-pink.webp","effects":[{"label":"Illusory pattern","icon":"icons/magic/air/air-burst-spiral-pink.webp","origin":"Actor.1gplsg6mKs6bPu5Y.Item.SaEU4xqz5h28NFAv","duration":{"seconds":null,"startTime":null,"combat":null,"rounds":null,"turns":1,"startRound":null,"startTurn":null},"disabled":false,"_id":"5Ngek5yCgmtyGDmX","changes":[{"key":"macro.itemMacro.GM","mode":0,"value":"","priority":20}],"tint":null,"transfer":false,"flags":{"dfreds-convenient-effects":{"description":""},"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""}}},{"label":"Charmed","icon":"icons/magic/air/air-burst-spiral-pink.webp","origin":"Actor.1gplsg6mKs6bPu5Y.Item.SaEU4xqz5h28NFAv","duration":{"seconds":null,"startTime":null,"combat":null,"rounds":10,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"8GPgFiVFefA56ELX","changes":[{"key":"StatusEffect","mode":0,"value":"Convenient Effect: Incapacitated","priority":20}],"tint":null,"transfer":false,"flags":{"dfreds-convenient-effects":{"description":""},"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":["isDamaged"]},"core":{"statusId":""}}}],"folder":null,"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1661787234072,"modifiedTime":1678762960485,"lastModifiedBy":"1VOK90B3fElXpLfM"}},{"_id":"1JkxeV2FLuJUnrCV","name":"Polymorph","ownership":{"default":0,"1VOK90B3fElXpLfM":3},"type":"spell","system":{"description":{"value":"<p>This spell transforms a creature that you can see within range into a new form. An unwilling creature must make a Wisdom saving throw to avoid the effect. The spell has no effect on a shapechanger or a creature with 0 hit points.</p><p>The transformation lasts for the duration, or until the target drops to 0 hit points or dies. The new form can be any beast whose challenge rating is equal to or less than the target's (or the target's level, if it doesn't have a challenge rating). The target's game statistics, including mental ability scores, are replaced by the statistics of the chosen beast. It retains its alignment and personality.</p><p>The target assumes the hit points of its new form. When it reverts to its normal form, the creature returns to the number of hit points it had before it transformed. If it reverts as a result of dropping to 0 hit points, any excess damage carries over to its normal form. As long as the excess damage doesn't reduce the creature's normal form to 0 hit points, it isn't knocked unconscious.</p><p>The creature is limited in the actions it can perform by the nature of its new form, and it can't speak, cast spells, or take any other action that requires hands or speech.</p><p>The target's gear melds into the new form. The creature can't activate, use, wield, or otherwise benefit from any of its equipment.</p>","chat":"","unidentified":""},"source":"PHB pg. 266","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"1","units":"hour"},"cover":null,"target":{"value":2,"width":null,"units":"","type":"creature"},"range":{"value":60,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"wis","dc":0,"scaling":"spell"},"level":4,"school":"trs","components":{"vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":true},"materials":{"value":"A caterpillar cocoon","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"always","prepared":false},"scaling":{"mode":"none","formula":""}},"sort":100001,"flags":{"core":{"sourceId":"Compendium.dnd5e.spells.04nMsTWkIFvkbXlY"},"midi-qol":{"effectActivation":false,"onUseMacroName":"[preItemRoll]ItemMacro,[preActiveEffects]ItemMacro"},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"autoFailFriendly":true,"autoSaveFriendly":false,"rollOther":false,"critOther":false,"offHandWeapon":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"ignoreTotalCover":false},"itemacro":{"macro":{"name":"Polymorph","type":"script","scope":"global","command":"/**\n * Polymorph automation macro for DnD5e\n * MIT License Copyright (c) 2022 Paulo Henrique (PH#2526)\n * Required modules: DAE, Item Macro, JB2A, Midi QoL, Sequencer\n * Usage: Run as ItemMacro via Midi's On Use Macros on the item sheet (\"preItemRoll\" and \"preActiveEffects\")\n *        Add \"auto fail friendly\" in the item description to have saves auto fail for friendly targets\n *        To enable the use of Twinned Spell, configure the item to allow targeting up to two tokens\n * To do: What if the compendium is locked/invisible for the player?\n *        After revertTransformation(), transfer eventual new items/AEs to the original actor; or\n *        ...create chat message informing eventual new items/AEs\n *        When zero HP, get damage from the workflow and transfer excess damage to the original actor\n *        if ( beast.cr > target.level ) return ui.notifications.warn(\"The selected beast CR must be equal to or less than the target's CR/level.\")\n *        Insert logic to unsetFlags after spell end\n * Issue: (see v10 system code before opening the issue; see new hooks)\n *        Unlinked tokens bug: execute revertOriginalForm() > close sheet > reopen sheet > incorrectly shows transformation data\n *        ...related: reverting unlinked token do not remove isPolymorphed flag; the flag gets removed after reload\n *        Add option to not transfer AEs depending on the TransformationOptions\n *        Align new token center with original token center\n *        Implement hook call for unlinked tokens and for revertOriginalForm()\n *        Wildshape should have \"keepFeats: true\" as default\n *        Linked token disposition are being changed (entity.js:1896 returns full prototype token data)\n *        Create dialog or chat message about new itens/AEs\n *        Pass all TransformationOptions to flags.dnd5e.transformOptions (useful for macro writers)\n *        Add option to not render sheet on transformInto() and revertOriginalForm() (useful for macro writers)\n *        ...and implement original.sheet.render(isRendered)) for unlinked actors\n *            See https://github.com/foundryvtt/dnd5e/issues/2205\n *        On linked tokens, revertOriginalForm() throws away transformed token data, like elevation, rotation etc.\n *        Related: Polymorphing does not revert correctly for NPC Actors: https://github.com/foundryvtt/dnd5e/issues/1917\n */\nconst effectName = \"Polymorphed\";\nconst defaultBeast = \"Giant Ape\";\nconst defaultPack = \"innocenti-worlds.monsters-custom\";\nconst lastArg = args[args.length - 1];\nconst collections = game.packs.filter(p => p.documentName === \"Actor\");\ncollections.push(game.actors);\nlet collection = game.packs.get(defaultPack) ?? game.packs.get(\"dnd5e.monsters\") ?? collections[0];\nconst sounds = [\n\t\"upload-player/feanor/transmutation.wav\",\n\t\"upload-player/feanor/counterspell-drain.ogg\",\n\t\"upload-player/feanor/boots-of-speed.wav\"\n];\nconst textures = [\n\t\"jb2a.magic_signs.circle.02.transmutation.intro.yellow\",\n\t\"jb2a.magic_signs.circle.02.transmutation.loop.yellow\",\n\t\"jb2a.magic_signs.circle.02.transmutation.outro.yellow\",\n\t\"jb2a.energy_strands.complete.blue.01\"\n];\n\nif ( args[0].macroPass === \"preItemRoll\" ) {\n\tconst beastUuids = {};\n\tconst targets = [...game.user.targets];\n\tif ( targets.length === 2 && targets[1].actor === targets[0].actor ) {\n\t\tui.notifications.warn(\"You may not target two linked tokens from the same actor. Choose only one of the tokens.\", {permanent: true});\n\t\treturn false;\n\t}\n\tfor ( const target of targets ) {\n\t\tconst beastId = await getUserOptions(target);\n\t\tif ( !beastId ) return false;\n\t\tbeastUuids[target.id] = collection.name === \"Actors\" ? collection.get(beastId).uuid : (await collection.getDocument(beastId)).uuid;\n\t}\n\tawait item.setFlag(\"world\", \"polymorph\", beastUuids);\n\tawait Sequencer.Preloader.preloadForClients([...sounds, ...textures]);\n}\n\nif ( args[0].macroPass === \"preActiveEffects\" ) {\n\tconst polymorphedTarget = args[0].targets.some(tokenDoc => tokenDoc.actor?.isPolymorphed);\n\tif ( polymorphedTarget ) await Sequencer.Helpers.wait(6000); // Waits for revertTransformation() (see Midi v10.0.15)\n\tif ( args[0].failedSaves.length ) new Sequence().sound(sounds[0]).delay(2500).play();\n\telse new Sequence().sound(sounds[0]).sound(sounds[1]).delay(4700).play();\n}\n\nif ( args[0] === \"on\" ) {\n\tconst tActor = MidiQOL.MQfromActorUuid(lastArg.actorUuid);\n\tif ( tActor.isPolymorphed ) return;\n\tconst tokens = tActor.isToken ? [tActor.token.object] : tActor.getActiveTokens(true);\n\tconst beastUuid = lastArg.efData.flags.dae.itemData.flags.world.polymorph[lastArg.tokenId];\n\tconst beast = await fromUuid(beastUuid);\n\tconst hook = Hooks._hooks[\"dnd5e.transformActor\"]?.some(fn => fn.name === \"disableActiveEffects\");\n\tif ( !hook ) Hooks.on(\"dnd5e.transformActor\", disableActiveEffects); // Currently only called for linked tokens\n\tpolymorph(tActor, tokens, beast);\n}\n\nif ( args[0] === \"off\" ) {\n\tconst tActor = MidiQOL.MQfromActorUuid(lastArg.actorUuid);\n\tconst effect = tActor.effects.get(lastArg.effectId);\n\tif ( !tActor.isPolymorphed || effect ) return;\n\tconst tokens = tActor.isToken ? [tActor.token.object] : tActor.getActiveTokens(true);\n\tconst original = game.actors.get(tActor.isToken ? tActor.token.data.actorId : tActor.getFlag(\"dnd5e\", \"originalActor\"));\n\trevertTransformation(tActor, tokens, original);\n}\n\n// Prompts the user to choose which beast to transform the target into\nfunction getUserOptions(target) {\n\treturn new Promise(async resolve => {\n\t\tconst dialog = new Dialog({\n\t\t\ttitle: \"Polymorph: Usage Configuration\",\n\t\t\tcontent: await getContent(target),\n\t\t\tbuttons: {\n\t\t\t\tconfirm: {\n\t\t\t\t\ticon: '<i class=\"fas fa-check\"></i>',\n\t\t\t\t\tlabel: \"Confirm\",\n\t\t\t\t\tcallback: html => resolve(html[0].querySelector(\"select#beast\")?.value)\n\t\t\t\t},\n\t\t\t\tcancel: {\n\t\t\t\t\ticon: '<i class=\"fas fa-times\"></i>',\n\t\t\t\t\tlabel: \"Cancel\"\n\t\t\t\t}\n\t\t\t},\n\t\t\tdefault: \"confirm\",\n\t\t\trender: html => {\n\t\t\t\thtml[0].querySelector(\"select#beast\")\n\t\t\t\t\t? html[0].querySelector(\"select#beast\").focus()\n\t\t\t\t\t: html[0].querySelector(\"select#source\").focus();\n\t\t\t\thtml[0].querySelector(\"select#source\").addEventListener(\"change\", reload);\n\t\t\t},\n\t\t\tclose: () => resolve(undefined)\n\t\t}).render(true);\n\t\tasync function reload(event) {\n\t\t\tcollection = event.target.value === \"Actors\" ? game.actors : game.packs.get(event.target.value);\n\t\t\tdialog.data.content = await getContent(target);\n\t\t\tdialog.render();\n\t\t}\n\t});\n}\n\n// Creates the dialog content based on the current selected collection\nasync function getContent(target) {\n\tlet beastEntries;\n\tif ( collection.name === \"CompendiumCollection\" ) {\n\t\tconst index = await collection.getIndex({fields: [\"data.details.type.value\"]});\n\t\tbeastEntries = index.filter(e => e.data?.details.type.value === \"beast\");\n\t} else {\n\t\tbeastEntries = collection.filter(a => a.data.data.details.type?.value === \"beast\");\n\t}\n\tconst sources = collections.map(c => {\n\t\tconst source = c.name === \"Actors\" ? c.name : c.collection;\n\t\tconst name = c.name === \"Actors\" ? \"World Actors\" : c.title;\n\t\treturn `<option value=\"${source}\"${source === collection.name || source === collection.collection ? \" selected\" : \"\"}>${name}</option>`;\n\t}).join(\"\");\n\tlet beastOptions = beastEntries.map(e => {\n\t\tconst beastId = collection.name === \"Actors\" ? e.id : e._id;\n\t\treturn `<option value=\"${beastId}\"${e.name === defaultBeast ? \" selected\" : \"\"}>${e.name}</option>`;\n\t}).join(\"\");\n\tlet content = `<table style=\"width:100%\"><tr>\n\t\t\t<th colspan=\"2\" style=\"font-weight:normal; font-size:1.4em; border-bottom:1px solid #7a7971\">Choose a beast for <b>${target.name}</b></th>\n\t\t</tr><tr>\n\t\t\t<td style=\"width:30%; text-align:center\">Source:</td>\n\t\t\t<td style=\"width:70%\"><select style=\"width:100%\" id=\"source\">${sources}</select></td>\n\t\t</tr><tr style=\"height:34.5px\">\n\t\t\t<td style=\"width:30%; text-align:center\">Name:</td>`;\n\tbeastOptions ?\n\t\tcontent += `<td style=\"width:70%\"><select style=\"width:100%\" id=\"beast\">${beastOptions}</select></td></tr></table>` :\n\t\tcontent += `<td style=\"width:70%\">No beasts in this collection</td></tr></table>`;\n\treturn content;\n}\n\n// Plays sound and visual effects and transforms the target into the selected beast\nasync function polymorph(actor, tokens, beast) {\n\tfor ( const token of tokens ) {\n\t\tconst energyStrandsSize = Math.max(token.data.width, beast.data.token.width) * 1.5;\n\t\tnew Sequence()\n\t\t\t.effect()\n\t\t\t\t.file(textures[0])\n\t\t\t\t.atLocation(token, {cacheLocation: true})\n\t\t\t\t.belowTokens()\n\t\t\t\t.size(token.data.width + 4, {gridUnits: true})\n\t\t\t\t.animateProperty(\"sprite\", \"rotation\", {from: 0, to: 180, duration: 2000, ease: \"easeInOutCubic\"})\n\t\t\t\t.delay(1, 2000)\n\t\t\t\t.fadeOut(200)\n\t\t\t\t.waitUntilFinished(-700)\n\t\t\t.effect()\n\t\t\t\t.file(textures[3])\n\t\t\t\t.atLocation(token)\n\t\t\t\t.attachTo(token)\n\t\t\t\t.size(energyStrandsSize, {gridUnits: true})\n\t\t\t\t.fadeIn(500)\n\t\t\t\t.duration(7000)\n\t\t\t\t.fadeOut(500)\n\t\t\t.effect()\n\t\t\t\t.file(textures[1])\n\t\t\t\t.atLocation(token, {cacheLocation: true})\n\t\t\t\t.belowTokens()\n\t\t\t\t.size(token.data.width + 4, {gridUnits: true})\n\t\t\t\t.rotate(180)\n\t\t\t\t.fadeIn(200)\n\t\t\t\t.fadeOut(200)\n\t\t\t\t.waitUntilFinished(-900)\n\t\t\t.effect()\n\t\t\t\t.file(textures[2])\n\t\t\t\t.atLocation(token, {cacheLocation: true})\n\t\t\t\t.belowTokens()\n\t\t\t\t.size(token.data.width + 4, {gridUnits: true})\n\t\t\t\t.rotate(180)\n\t\t\t.play();\n\t}\n\tawait new Sequence().wait(7000, 9000).sound(sounds[2]).play();\n\tif ( !actor.isToken ) Hooks.once(\"preCreateActor\", preventSheetRender);\n\tHooks.on(\"preUpdateToken\", centerTransformedToken);\n\tif ( actor.isToken ) { // \"dnd5e.transformActor\" hook is not called for unlinked tokens\n\t\tconst originalActor = game.actors.get(actor.id);\n\t\tconst data = {effects: actor.effects.toObject()};\n\t\tdata.effects = disableActiveEffects(actor, null, data);\n\t\tdata.effects = disableActiveEffects(originalActor, null, data);\n\t\tawait actor.updateEmbeddedDocuments(\"ActiveEffect\", data.effects);\n\t\tawait actor.transformInto(beast);\n\t} else {\n\t\tawait actor.transformInto(beast);\n\t\tconst effect = actor.effects.get(lastArg.effectId);\n\t\teffect.delete(); // Removes Polymorph effect from the original actor\n\t}\n\tHooks.off(\"preUpdateToken\", centerTransformedToken);\n}\n\n// Plays sound and visual effects and reverts the target to its original form\nasync function revertTransformation(actor, tokens, original) {\n\tfor ( const token of tokens ) {\n\t\tconst energyStrandsSize = Math.max(token.data.width, original.data.token.width) * 1.5;\n\t\tnew Sequence()\n\t\t\t.effect()\n\t\t\t\t.file(textures[3])\n\t\t\t\t.atLocation(token)\n\t\t\t\t.attachTo(token)\n\t\t\t\t.size(energyStrandsSize, {gridUnits: true})\n\t\t\t\t.delay(1, 2000)\n\t\t\t\t.fadeIn(500)\n\t\t\t\t.duration(4000)\n\t\t\t\t.fadeOut(500)\n\t\t\t.play();\n\t}\n\tawait new Sequence().wait(2000, 4000).sound(sounds[2]).play();\n\tHooks.on(\"preUpdateToken\", centerTransformedToken);\n\tif ( actor.isToken ) {\n\t\tawait actor.revertOriginalForm();\n\t\tactor.setFlag(\"dnd5e\", \"isPolymorphed\", false); // Workaround for revertOriginalForm() bug (see issue)\n\t} else {\n\t\tawait actor.sheet.close();\n\t\tawait actor.revertOriginalForm();\n\t}\n\tHooks.off(\"preUpdateToken\", centerTransformedToken);\n}\n\n// Disables Active Effects depending on the chosen transformation options\nfunction disableActiveEffects(actor, _beast, data, options={}) {\n\tdata.effects = data.effects.map(eData => {\n\t\tif ( eData.label === effectName || eData.label === \"Concentrating\" ) return eData;\n\t\tlet source = MidiQOL.MQFromUuid(eData.origin);\n\t\tif ( source instanceof TokenDocument ) source = source.actor;\n\t\tif ( !options.keepFeats && !options.keepClass ) { // Checking keepClass as a workaround, as Wildshape sets keepFeats as false (see issue)\n\t\t\tif ( source === actor || (source?.parent === actor && [\"background\",\"class\",\"feat\",\"subclass\"].includes(source?.type)) ) {\n\t\t\t\teData.disabled = true;\n\t\t\t}\n\t\t}\n\t\tif ( !options.keepItems ) {\n\t\t\tif ( source?.parent === actor && [\"backpack\",\"equipment\",\"loot\",\"tool\",\"weapon\"].includes(source?.type) ) {\n\t\t\t\teData.disabled = true;\n\t\t\t}\n\t\t}\n\t\treturn eData;\n\t});\n\treturn data.effects;\n}\n\n// Workaround to prevent the transformation method from opening the actor sheet\nfunction preventSheetRender(_actor, _data, options) {\n\toptions.renderSheet = false;\n}\n\n// Workaround to align the center of the original and transformed tokens\nfunction centerTransformedToken(tokenDoc, changes) {\n\t// Adjust linked token disposition and has vision?\n\tif ( !(\"width\" in changes) ) return;\n\tconst grid = canvas.grid.size;\n\tchanges.x = tokenDoc.data.x - (grid / 2) * (changes.width - tokenDoc.data.width);\n\tchanges.y = tokenDoc.data.y - (grid / 2) * (changes.width - tokenDoc.data.width);\n}","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}}},"img":"icons/creatures/amphibians/bullfrog-glowing-green.webp","effects":[{"label":"Polymorphed","icon":"icons/creatures/amphibians/bullfrog-glowing-green.webp","origin":"Actor.1gplsg6mKs6bPu5Y.Item.1JkxeV2FLuJUnrCV","duration":{"seconds":3600,"startTime":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"eMSbFwnPHjZElxu8","changes":[{"key":"macro.itemMacro.GM","mode":0,"value":"","priority":20}],"tint":null,"transfer":false,"flags":{"dfreds-convenient-effects":{"description":""},"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":["zeroHP"]},"core":{"statusId":""}}}],"folder":null,"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1661787234052,"modifiedTime":1678842331182,"lastModifiedBy":"1VOK90B3fElXpLfM"}},{"name":"Bloodwell Vial +1","type":"equipment","system":{"description":{"value":"<p><em>Wondrous item, uncommon (requires attunement by a sorcerer)</em></p><p>To attune to this vial, you must place a few drops of your blood into it. The vial can't be opened while your attunement to it lasts. If your attunement to the vial ends, the contained blood turns to ash. You can use the vial as a spellcasting focus for your spells while wearing or holding it, and you gain a +1 bonus to spell attack rolls and to the saving throw DCs of your sorcerer spells.</p><p>In addition, when you roll any Hit Dice to recover hit points while you are carrying the vial, you can regain 5 sorcery points. This property of the vial can't be used again until the next dawn.</p>","chat":"","unidentified":""},"source":"TCE pg. 122","q