/assets/tokens/npc%27s/humans/with-shadows/Courtesan1.png\";\nconst hoveredToken = canvas.tokens.placeables.find(t => t._hover);\nif ( hoveredToken ) illusion = hoveredToken.data.img;\n\nasync function disguise() {\n\tconst params = [{\n\t\tfilterType: \"polymorph\",\n\t\tfilterId: \"disguise-self\",\n\t\tautoDestroy: true,\n\t\timagePath: illusion,\n\t\ttype: 9,\n\t\tanimated: {\n\t\t\tprogress: {\n\t\t\t\tanimType: \"halfCosOscillation\",\n\t\t\t\tval1: 0,\n\t\t\t\tval2: 100,\n\t\t\t\tloops: 1,\n\t\t\t\tloopDuration: 1500\n\t\t\t}\n\t\t}\n\t}];\n\ttoken.TMFXaddUpdateFilters(params);\n\t// TokenMagic.addUpdateFilters(token, params);\n\tawait new Promise(r => setTimeout(r, 1400));\n\ttoken.document.update({img: illusion});\n}\n\nif ( args[0].tag === \"OnUse\" && effect ) {\n\tif ( game.keyboard.isModifierActive(\"Shift\") ) effect.delete();\n\telse if ( token ) disguise();\n\telse ui.notifications.warn(\"Disguise Self already active.\");\n\treturn false;\n}\n\nif ( args[0] === \"on\" && token ) disguise();\n\nif ( args[0] === \"off\" && token ) {illusion = actor.data.token.img; disguise();}","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}}},"img":"modules/plutonium/media/icon/spell/phb-disguise-self.webp","effects":[{"label":"Illusory disguise","icon":"modules/plutonium/media/icon/spell/phb-disguise-self.webp","origin":"Actor.1gplsg6mKs6bPu5Y.Item.X7p4K62BOA9HUjMb","duration":{"seconds":3600,"startTime":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"mLEhtmHxKwFwCy4w","changes":[{"key":"macro.itemMacro","mode":0,"value":"","priority":20}],"tint":null,"transfer":false,"flags":{"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDu