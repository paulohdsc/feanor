vation":false,"onUseMacroParts":{"items":{"0":{"macroName":"ItemMacro","option":"preSave"},"1":{"macroName":"ItemMacro","option":"preActiveEffects"}}}},"itemacro":{"macro":{"name":"Hypnotic Pattern","type":"script","scope":"global","command":"/**\n * Hypnotic Pattern automation macro for DnD5e\n * MIT License Copyright (c) 2022 Paulo Henrique (PH#2526)\n * Required modules: Advanced Macros, DAE, Item Macro, JB2A, Midi QoL, Sequencer, Times Up, Token Magic FX\n * Usage: Run as ItemMacro via Midi's On Use Macro on the item sheet (\"preSave\" and \"preActiveEffects\")\n * \t\t  The \"Illusory Pattern\" Active Effect must be removed by DAE or disabled for the tile to be removed\n */\nconst patternImage = \"upload-player/feanor/hypnotic-pattern.webp\";\nif ( args[0].tag === \"OnUse\" ) {\n\tif ( args[0].macroPass === \"preSave\" ) {\n\t\tconst illusoryPattern = item.effects.find(e => e.label === \"Illusory pattern\");\n\t\tDAE.doEffects(item, true, [actor], {whisper: false, effectsToApply: [illusoryPattern.id]});\n\t\tawait new Promise(r => setTimeout(r, 7000));\t// Teaser delay in saving throw\n\t}\n\tif ( args[0].macroPass === \"preActiveEffects\" ) {\n\t\tconst targets = args[0].failedSaves.filter(token => !token.actor.system.traits.ci.value.has(\"charmed\"));\n\t\tconst charmed = item.effects.find(e => e.label === \"Charmed\");\n\t\tDAE.doEffects(item, true, targets, {whisper: false, effectsToApply: [charmed.id]});\n\t\tconst workflow = MidiQOL.Workflow.getWorkflow(args[0].uuid);\n\t\tawait workflow.removeEffectsButton();\n\t\treturn {haltEffectsApplication: true};\n\t}\n}\n\nif ( args[0] === \"on\" ) {\n\tconst grid = canvas.grid.size;\n\tconst template = canvas.templates.placeables[canvas.templates.placeables.length - 1];\n\tconst templateCenter = {x: template.x + grid * 3, y: template.y + grid * 3};\n\tconst tileData = JSON.parse(`{\"img\":\"${patternImage}\",\"width\":${grid*6},\"height\":${grid*6},\"x\":${template.x},\"y\":${template.y},\"overhead\":true,\"occlusion\":{\"alpha\":0.4,\"mode\":3},\"flags\":{\"tokenmagic\":{\"filters\":[{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"mix66eq324lh0k5y\",\"tmFilterType\":\"glow\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"glow\",\"filterId\":\"hypnoticPattern\",\"color\":773372,\"outerStrength\":2,\"animated\":{\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":773372,\"val2\":16734199}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"mix66eq324lh0k5y\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"uiqm25hwruucpf4q\"}}},{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"ro0gghrmt9bhryoj\",\"tmFilterType\":\"distortion\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"distortion\",\"filterId\":\"hypnoticPattern\",\"animated\":{\"maskSpriteX\":{\"speed\":0.05,\"animType\":\"move\"},\"maskSpriteY\":{\"speed\":0.07,\"animType\":\"move\"}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"ro0gghrmt9bhryoj\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"ok7xjbechdh7p8vf\"}}},{\"tmFilters\":{\"tmFilterId\":\"hypnoticPattern\",\"tmFilterInternalId\":\"7jbcq40dijxw8kal\",\"tmFilterType\":\"shadow\",\"tmFilterOwner\":\"JfEEYsevt40eKS09\",\"tmParams\":{\"filterType\":\"shadow\",\"filterId\":\"hypnoticPattern\",\"rotation\":45,\"distance\":70,\"alpha\":0.7,\"color\":212299,\"animated\":{\"blur\":{\"loopDuration\":2000,\"animType\":\"cosOscillation\",\"val1\":1,\"val2\":5},\"rotation\":{\"loopDuration\":4000,\"animType\":\"sinOscillation\",\"val1\":40,\"val2\":50},\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":212299,\"val2\":4987466}},\"rank\":null,\"enabled\":true,\"placeableId\":\"C6Q7NWme70K1zkPZ\",\"filterInternalId\":\"7jbcq40dijxw8kal\",\"filterOwner\":\"JfEEYsevt40eKS09\",\"placeableType\":\"Tile\",\"updateId\":\"mjpre97fk5ry1kc6\"}}}]}}}`);\n\ttemplate.document.delete();\n\tawait new Sequence()\n\t\t.effect()\n\t\t\t.file(\"jb2a.magic_signs.circle.02.illusion.intro.purple\")\n\t\t\t.scale(.75)\n\t\t\t.belowTokens()\n\t\t\t.atLocation(templateCenter)\n\t\t.wait(2600)\n\t\t.play();\n\tcanvas.scene.createEmbeddedDocuments(\"Tile\", [tileData]);\n}\n\nif ( args[0] === \"off\" ) {\n\tconst lastArg = args.at(-1); // args[args.length - 1];\n\tconst actor = fromUuidSync(lastArg.actorUuid);\n\tconst effect = actor?.effects.get(lastArg.effectId);\n\tif ( !effect ) {\n\t\tconst tile = canvas.tiles.placeables.find(t => t.document.texture.src === patternImage);\n\t\tif ( tile ) tile.document.delete();\n\t} else DAE.deleteActiveEffect(lastArg.actorUuid, lastArg.origin);\n}\n\n// Tile's filters backup: '[{\"filterType\":\"glow\",\"filterId\":\"hypnoticPattern\",\"color\":773372,\"outerStrength\":2,\"animated\":{\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":773372,\"val2\":16734199}}},{\"filterType\":\"distortion\",\"filterId\":\"hypnoticPattern\",\"animated\":{\"maskSpriteX\":{\"speed\":0.05,\"animType\":\"move\"},\"maskSpriteY\":{\"speed\":0.07,\"animType\":\"move\"}}},{\"filterType\":\"shadow\",\"filterId\":\"hypnoticPattern\",\"rotation\":45,\"distance\":70,\"alpha\":0.7,\"color\":212299,\"animated\":{\"blur\":{\"loopDuration\":2000,\"animType\":\"cosOscillation\",\"val1\":1,\"val2\":5},\"rotation\":{\"loopDuration\":4000,\"animType\":\"sinOscillation\",\"val1\":40,\"val2\":50},\"color\":{\"loopDuration\":5000,\"animType\":\"colorOscillation\",\"val1\":212299,\"val2\":4987466}}}]'","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"autoFailFriendly":false,"autoSaveFriendly":false,"rollOther":false,"critOther":false,"offHandWeapon":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"ignoreTotalCover":false}},"img":"icons/magic/air/air-burst-spiral-pink.webp","effects":[{"label":"Illusory pattern","icon":"icons/magic/air/air-burst-spiral-pink.webp","origin":"Actor.XZwphshCVIpHgF21.Item.SaEU4xqz5h28NFAv","duration":{"seconds":null,"startTime":null,"combat":null,"rounds":null,"turns":1,"startRound":null,"startTurn":null},"disabled":false,"_id":"5Ngek5yCgmtyGDmX","changes":[{"key":"macro.itemMacro.GM","mode":0,"value":"","priority":20}],"tint":null,"transfer":false,"flags":{"dfreds-convenient-effects":{"description":""},"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""}}},{"label":"Charmed","icon":"icons/magic/air/air-burst-spiral-pink.webp","origin":"Actor.XZwphshCVIpHgF21.Item.SaEU4xqz5h28NFAv","duration":{"seconds":null,"startTime":null,"combat":null,"rounds":10,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"8GPgFiVFefA56ELX","changes":[{"key":"macro.CE","mode":0,"value":"Incapacitated","priority":20}],"tint":null,"transfer":false,"flags":{"dae":{"specialDuration":["isDamaged"],"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none"},"dfreds-convenient-effects":{"description":""},"core":{"statusId":""}}}],"folder":null,"_stats":{"systemId":"dnd5e","systemVersion":"2.3.1","coreVersion":"10.303","createdTime":1661787234072,"modifiedTime":1692998069340,"lastModifiedBy":"1VOK90B3fElXpLfM"}},{"_id":"fnfwPZhrZZ5vVF85","name":"Bloodwell Vial +1","type":"equipment","system":{"description":{"value":"<p><em>Wondrous item, uncommon (requires attunement by a sorcerer)</em></p><p>To attune to this vial, you must place a few drops of your blood into it. The vial can't be opened while your attunement to it lasts. If your attunement to the vial ends, the contained blood turns to ash. You can use the vial as a spellcasting focus for your spells while wearing or holding it, and you gain a +1 bonus to spell attack rolls and to the saving throw DCs of your sorcerer spells.</p><p>In addition, when you roll any Hit Dice to recover hit points while you are carrying the vial, you can regain 5 sorcery points. This property of the vial can't be used again until the next dawn.</p>","chat":"","unidentified":""},"source":"TCE pg. 122","quantity":1,"weight":0.1,"price":{"value":12000,"denomination":"gp"},"attunement":2,"equipped":true,"rarity":"uncommon","identified":true,"activation":{"type":"special","cost":null,"condition":"When you roll any Hit Dice to recover hit points while you are carrying the vial"},"duration":{"value":"","units":""},"cover":null,"crewed":false,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":"1","per":"day","recovery":""},"consume":{"type":"charges","target":"OlPzhisxQ0TpOzbc","amount":-5},"ability":null,"actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":null,"type":"trinket","dex":null},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"baseItem":"","speed":{"value":null,"conditions":""},"strength":0,"stealth":false,"proficient":0},"img":"icons/equipment/neck/pendant-faceted-red.webp","effects":[{"label":"+1 to spell attacks and save DC","icon":"icons/equipment/neck/pendant-faceted-red.webp","origin":"Actor.XZwphshCVIpHgF21.Item.fnfwPZhrZZ5vVF85","duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"TnXmHnjfb0QaIsbB","changes":[{"key":"system.bonuses.spell.attack","mode":2,"value":"1","priority":20},{"key":"system.bonuses.spell.dc","mode":2,"value":"+1","priority":20}],"tint":null,"transfer":true,"flags":{"dfreds-convenient-effects":{"description":""},"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""}}}],"folder":null,"sort":0,"ownership":{"default":0,"1VOK90B3fElXpLfM":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1672454949512,"modifiedTime":1692751447995,"lastModifiedBy":"1VOK90B3fElXpLfM"}},{"_id":"G5AeE0qSqY8Erab4","name":"Traveler's Clothes","type":"equipment","img":"icons/equipment/chest/shirt-collared-brown.webp","system":{"description":{"value":"<p>A set of clothes worn by travellers.</p>","chat":"","unidentified":""},"source":"PHB pg. 150","quantity":1,"weight":4,"price":{"value":2,"denomination":"gp"},"attunement":0,"equipped":true,"rarity":"","identified":true,"activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"crewed":false,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":"","amount":null},"ability":null,"actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":null,"type":"clothing","dex":null},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"baseItem":"","speed":{"value":null,"conditions":""},"strength":0,"stealth":false,"proficient":0},"effects":[],"folder":null,"sort":846875,"ownership":{"default":0,"1VOK90B3fElXpLfM":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1661787233666,"modifiedTime":1692658054358,"lastModifiedBy":"1VOK90B3fElXpLfM"}},{"name":"Wand of Magic Missiles","type":"equipment","system":{"description":{"value":"<p>This wand has 7 charges. While holding it, you can use an action to expend 1 or more of its charges to cast the @Compendium[dnd5e.spells.41JIhpDyM9Anm7cs]{Magic Missile} spell from it. For 1 charge, you cast the 1st-level version of the spell. You can increase the spell slot level by one for each additional charge you expend.</p><p>The wand regains 1d6 + 1 expended charges daily at dawn. If you expend the wand's last charge, roll a d20. On a 1, the wand crumbles into ashes and is destroyed.</p>","chat":"","unidentified":""},"source":"DMG pg. 211","quantity":1,"weight":1,"price":{"value":8000,"denomination":"gp"},"attunement":0,"equipped":true,"rarity":"uncommon","identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":"inst"},"cover":null,"crewed":false,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":120,"long":null,"units":"ft"},"uses":{"value":7,"max":"7","per":"charges","recovery":"1d6 + 1"},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"util","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":null,"type":"trinket","dex":null},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"baseItem":"","speed":{"value":null,"conditions":""},"strength":0,"stealth":false,"proficient":0},"_id":"XmXq94F8jWVJ6RdA","img":"icons/weapons/staves/staff-ornate-engraved-blue.webp","effects":[],"folder":null,"sort":850000,"ownership":{"default":0,"1VOK90B3fElXpLfM":3},"flags":{"midi-qol":{"onUseMacroName":"[preItemRoll]ItemMacro","effectActivation":false,"onUseMacroParts":{"items":{"0":{"macroName":"ItemMacro","option":"preItemRoll"}}}},"itemacro":{"macro":{"name":"Wand of Magic Missiles","type":"script","scope":"global","command":"/**\n * Wand of Magic Missiles automation macro for DnD5e\n * MIT License Copyright (c) 2022 Paulo Henrique (PH#2526)\n * Required modules: Item Macro, JB2A, Midi QoL, Sequencer\n * Usage: Run as ItemMacro via Midi's On Use Macro on the item sheet (\"preItemRoll\")\n * To do: All variables to the top\n * \t\t  if ( !item.data.data.uses.value ) ChatMessage.create({content: \"Spent last charge; roll a d20\"});\n * Notes:\n */\nconst mediaFiles = [\n    \"jb2a.magic_missile.purple\",\n    \"upload-player/feanor/magic-missile/magic-missile-1.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-2.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-3.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-4.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-5.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-6.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-7.mp3\",\n    \"upload-player/feanor/magic-missile/magic-missile-8.mp3\"\n];\nconst charges = await selectCharges();\nconst spellLevel = Number(charges);\nconst missilesTotal = spellLevel + 2;\nlet missilesLeft = missilesTotal;\nlet aimedMissiles = {};\n\nactivateTool(\"target\");\nSequencer.Preloader.preloadForClients(mediaFiles, false);\nwhile ( missilesLeft ) await setTargets().catch(err => {activateTool(\"select\"); throw err;});\nactivateTool(\"select\");\n\nawait item.displayCard();\nlet damageRoll = new Roll(\"1d4+1\");\nawait damageRoll.toMessage({\n\tspeaker: {alias: actor.name},\n\tflavor: `Spent <b>${charges} charge${charges < 2 ? \"\" : \"s\"}</b> to cast <b>${missilesTotal} missiles</b>`\n});\n\n// DSN API: added a \"waitFor3DAnimationByMessageID\" function that resolves a promise when a 3D animation stops\n\nconst reroll = await confirmEmpoweredSpell();\nif ( reroll ) {\n\tawait reroll.toMessage({\n\t\tspeaker: {alias: actor.name},\n\t\tflavor: \"Spent <b>1 sorcery point</b> to reroll the damage\"\n\t});\n\tdamageRoll = reroll;\n}\n\nawait castMissiles(\"singleConcentrationSave\", \"strikeSuccessively\");\ngame.user.updateTokenTargets();\nitem.update({\"system.uses.value\": item.system.uses.value - spellLevel});\nthrow \"Wand of Magic Missiles: Missiles Cast\";\t// Alternative: return false;\n\n/* -------------------------------------------- */\n\n// Prompts the user to choose how many charges to expend\nasync function selectCharges() {\n\treturn new Promise((resolve, reject) => {\n\t\tconst chargesLeft = item.system.uses.value;\n\t\tconst title = \"Wand of Magic Missiles: Usage Configuration\";\n\t\tlet content = '<p style=\"text-align:center\">No charges left to expend</p>';\n\t\tlet buttons = {};\n\t\tif ( chargesLeft ) {\n\t\t\tlet options = \"\";\n\t\t\tfor ( let i = 1; i <= chargesLeft; i++ ) options += `<option value=${i}>${i}</option>`;\n\t\t\tcontent = `<form style=\"text-align:center\">\n\t\t\t\t<label>Charges to expend:</label>\n\t\t\t\t<select style=\"width:50px; margin:0 0 5px 10px\">${options}</select>\n\t\t\t</form>`;\n\t\t\tbuttons.cast = {\n\t\t\t\t// Update to fontawesome v6 on Foundry v10: icon: '<i class=\"fas fa-wand-sparkles\"></i>',\n\t\t\t\ticon: '<i class=\"fas fa-magic\"></i>',\n\t\t\t\tlabel: \"Cast\",\n\t\t\t\tcallback: html => resolve(html[0].querySelector(\"select\").value)\n\t\t\t};\n\t\t}\n\t\tnew Dialog({\n\t\t\ttitle, content, buttons,\n\t\t\tdefault: chargesLeft ? \"cast\" : null,\n\t\t\trender: html => html.find(\"select\").focus(),\n\t\t\tclose: () => reject(new Error(\"The Dialog was closed without a choice being made.\"))\n\t\t},{top: 0, width: 350}).render(true);\n\t});\n}\n\n// Activate a given tool from the tokens layer controls\nfunction activateTool(toolName) {\n\t// ui.controls.controls.find(c => c.name === \"token\").activeTool = toolName;\n\t// canvas.tokens.activate();\n\t// ui.controls.render();\n\tif ( canvas.tokens.active ) ui.controls?.initialize({tool: toolName});\n        else canvas.tokens.activate({tool: toolName});\n        // Issue Core: run ui.controls.initialize() when token layer is already active\n}\n\n// Prompts the user to choose how many missiles to assign to which targets\nasync function setTargets() {\n\treturn new Promise((resolve, reject) => {\n\t\tnew Dialog({\n\t\t\ttitle: \"Wand of Magic Missiles: Usage Configuration\",\n\t\t\tcontent: `<p style=\"text-align:center\">Select your targets | Missiles left: <b>${missilesLeft}</b></p>`,\n\t\t\tbuttons: {\n\t\t\t\tone: {\n\t\t\t\t\ticon: '<i class=\"fas fa-check\"></i>',\n\t\t\t\t\tlabel: \"1 missile\",\n\t\t\t\t\tcallback: () => resolve(aimMissiles(1))\n\t\t\t\t},\n\t\t\t\ttwo: {\n\t\t\t\t\ticon: '<i class=\"fas fa-check-double\"></i>',\n\t\t\t\t\tlabel: \"2 missiles\",\n\t\t\t\t\tcallback: () => resolve(aimMissiles(2))\n\t\t\t\t},\n\t\t\t\tall: {\n\t\t\t\t\ticon: '<i class=\"fas fa-bullseye\"></i>',\n\t\t\t\t\tlabel: \"All missiles\",\n\t\t\t\t\tcallback: () => resolve(aimMissiles(\"all\"))\n\t\t\t\t}\n\t\t\t},\n\t\t\tdefault: \"all\",\n\t\t\trender: html => html.find(\"button.all\").focus(),\n\t\t\tclose: () => reject(new Error(\"Wand of Magic Missiles: The Dialog was closed without being accepted\"))\n\t\t},{top: 0, width: 350}).render(true);\n\t});\n}\n\n// Validates and saves the missile aiming choices of the user\nfunction aimMissiles(quantity) {\n\tconst targets = Array.from(game.user.targets);\n\tif ( !targets.length ) return ui.notifications.warn(\"Choose at least one target.\");\n\tif ( token ) {\n\t\tconst range = item.system.range.value;\n\t\tfor ( const target of targets ) {\n\t\t\tconst distance = MidiQOL.getDistance(token, target);\n\t\t\tif ( distance > range ) return ui.notifications.warn(`One target is ${distance} units away and your range is only ${range}.`);\n\t\t}\n\t}\n\tconst missiles = quantity === \"all\" ? Math.floor(missilesLeft / targets.length) : quantity;\n\tif ( !missiles || missiles * targets.length > missilesLeft ) {\n\t\treturn ui.notifications.warn(\"Not enough missiles, you must choose another setup.\");\n\t} else {\n\t\tmissilesLeft -= missiles * targets.length;\n\t\tfor ( const target of targets ) {\n\t\t\tconst targetUuid = target.document.uuid;\n\t\t\tif ( !(targetUuid in aimedMissiles) ) aimedMissiles[targetUuid] = missiles;\n\t\t\telse aimedMissiles[targetUuid] += missiles;\n\t\t}\n\t}\n}\n\n// Prompts the user to choose whether or not to use Empowered Spell metamagic\nasync function confirmEmpoweredSpell() {\n\tconst empoweredSpell = actor.items.getName(\"Metamagic: Empowered Spell\");\n\tconst sorceryPoints = actor.items.getName(\"Sorcery Points\");\n\tconst sorceryPointsLeft = sorceryPoints?.system.uses.value;\n\tif ( !empoweredSpell || !sorceryPointsLeft ) return false;\n\treturn Dialog.confirm({\n\t\ttitle: \"Empowered Spell: Confirmation Dialog\",\n\t\tcontent: '<p style=\"text-align:center\">Spend 1 sorcery point to reroll the damage?</p>',\n\t\tyes: () => {\n\t\t\tsorceryPoints.update({\"system.uses.value\": sorceryPointsLeft - 1});\n\t\t\tempoweredSpell.displayCard();\n\t\t\treturn new Roll(\"1d4+1\");\n\t\t},\n\t\tdefaultYes: false,\n\t\trender: html => html.find(\"button.no\").focus(),\n\t\toptions: {top: 0, width: 350}\n\t});\n}\n\n// to launch only one missile per target per roll??\n// if ( game.dice3d?.isEnabled() ) await new Promise(r => setTimeout(r, 2000));\n\n// Rolls damage for each missile and plays sound and visual effects\nasync function castMissiles(saveMode, fxMode) {\n\tlet dmgTargets = Object.keys(aimedMissiles);\n\tlet aimedMissilesClone = {...aimedMissiles}; // Clones the object using spread syntax (for nested data, use deepClone() instead)\n\tlet fxTargets = Object.keys(aimedMissilesClone);\n\tlet spell;\n\n\t// Rolls one Concentration saving throw for each missile\n\t// RAI: https://twitter.com/jeremyecrawford/status/716012166101401600\n\tif ( saveMode === \"multiConcentrationSaves\" ) {\n\t\tspell = createSpell(damageRoll.total);\n\t\twhile ( dmgTargets.length ) {\n\t\t\tawait MidiQOL.completeItemRoll(spell, {targetUuids: dmgTargets});\n\t\t\tfor ( const target in aimedMissiles ) {\n\t\t\t\taimedMissiles[target]--;\n\t\t\t\tif ( !aimedMissiles[target] ) delete aimedMissiles[target];\n\t\t\t}\n\t\t\tdmgTargets = Object.keys(aimedMissiles);\n\t\t}\n\t}\n\n\t// Rolls one Concentration saving throw for each target (house rule)\n\t// Groups targets according to how many missiles were aimed at each of them\n\tif ( saveMode === \"singleConcentrationSave\" ) {\n\t\tconst missilesPerTarget = Object.values(aimedMissiles);\n\t\tconst maxMissilesPerTarget = Math.max(...missilesPerTarget);\n\t\tfor ( let missiles = 1; missiles <= maxMissilesPerTarget; missiles++ ) {\n\t\t\tconst midiTargets = dmgTargets.filter(target => aimedMissiles[target] === missiles);\n\t\t\tif ( !midiTargets.length ) continue;\n\t\t\tspell = createSpell(damageRoll.total * missiles);\n\t\t\tawait MidiQOL.completeItemRoll(spell, {targetUuids: midiTargets});\n\t\t}\n\t}\n\n\t// Strikes missiles simultaneously at all targets\n\t// RAW, but less visually appealing with the effects available\n\tif ( fxMode === \"strikeSimultaneously\" ) {\n\t\tconst seq = new Sequence();\n\t\tconst missilesNumber = Object.values(aimedMissilesClone).reduce((acc, value) => acc + value, 0);\n\t\tfor ( const target of fxTargets ) {\n\t\t\tseq.effect()\n\t\t\t\t.file(\"jb2a.magic_missile.purple\")\n\t\t\t\t.atLocation(token)\n\t\t\t\t.stretchTo(target)\n\t\t\t\t.randomizeMirrorY()\n\t\t\t\t.repeats(aimedMissilesClone[target]);\n\t\t}\n\t\tseq.sound()\n\t\t\t.file(\"upload-player/feanor/magic-missile/*.mp3\")\n\t\t\t.repeats(missilesNumber, 150);\n\t\tawait seq.play();\n\t}\n\n\t// Strikes missiles successively at random targets\n\t// Not RAW, just for fun, with no mechanical consequence\n\tif ( fxMode === \"strikeSuccessively\" ) {\n\t\twhile ( fxTargets.length ) {\n\t\t\tconst target = fxTargets[Math.floor(Math.random() * fxTargets.length)];\n\t\t\tawait new Sequence()\n\t\t\t\t.effect()\n\t\t\t\t\t.file(\"jb2a.magic_missile.purple\")\n\t\t\t\t\t.atLocation(token)\n\t\t\t\t\t.stretchTo(target)\n\t\t\t\t\t.randomizeMirrorY()\n\t\t\t\t.sound()\n\t\t\t\t\t.file(\"upload-player/feanor/magic-missile/*.mp3\")\n\t\t\t\t.wait(300)\n\t\t\t\t.play();\n\t\t\taimedMissilesClone[target]--;\n\t\t\tif ( !aimedMissilesClone[target] ) delete aimedMissilesClone[target];\n\t\t\tfxTargets = Object.keys(aimedMissilesClone);\n\t\t}\n\t}\n}\n\n// Creates a temporary Magic Missile item with the provided damage\n// The item is configured to disable the automatic recognition of Automated Animations\nfunction createSpell(damage) {\n\tconst magicMissileData = `{\"name\":\"Magic Missile\",\"type\":\"spell\",\"img\":\"icons/magic/fire/projectile-meteor-salvo-light-pink.webp\",\"system\":{\"description\":{\"value\":\"<p>You create three glowing darts of magical force. Each dart hits a creature of your choice that you can see within range. A dart deals 1d4 + 1 force damage to its target. The darts all strike simultaneously, and you can direct them to hit one creature or several.</p><p><strong>Higher Levels. </strong>When you cast this spell using a spell slot of 2nd level or higher, the spell creates one more dart for each slot level above 1st.</p>\"},\"activation\":{\"type\":\"none\",\"cost\":null},\"duration\":{\"units\":\"inst\"},\"target\":{\"type\":\"creature\"},\"range\":{\"value\":120,\"units\":\"ft\"},\"actionType\":\"util\",\"damage\":{\"parts\":[[\"${damage}\",\"force\"]]},\"level\":${spellLevel},\"school\":\"evo\",\"components\":{\"vocal\":true,\"somatic\":true},\"preparation\":{\"mode\":\"atwill\"}},\"flags\":{\"autoanimations\":{\"version\":5,\"isEnabled\":false}}}`;\n\treturn new CONFIG.Item.documentClass(JSON.parse(magicMissileData), {parent: actor});\n}","author":"1VOK90B3fElXpLfM","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}}},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.0","coreVersion":"10.303","createdTime":1672455095509,"modifiedTime":1691884874297,"lastM